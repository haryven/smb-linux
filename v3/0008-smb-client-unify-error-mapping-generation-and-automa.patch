index 000000000000..a65c954ab3f1
--- /dev/null
+++ b/fs/smb/client/gen_cifs_maps.pl
@@ -0,0 +1,85 @@
+#!/usr/bin/perl -w
+# SPDX-License-Identifier: GPL-2.0-or-later
+#
+# Unified script to generate CIFS/SMB error mapping tables.
+#
+use strict;
+
+my $mode = shift || die "Usage: $0 <mode> <in-file> <out-file>";
+my $in_file = shift || die "Usage: $0 <mode> <in-file> <out-file>";
+my $out_file = shift || die "Usage: $0 <mode> <in-file> <out-file>";
+
+my @list = ();
+
+open(my $in, "<", $in_file) or die "Cannot open $in_file: $!";
+
+if ($mode eq "smb1-nt") {
+    # Logic from generate_smb1_maperror.pl
+    while (<$in>) {
+        if (/^\s*#define\s+(NT_STATUS_[A-Za-z0-9_]+)\s+(.+?)\s*\/\/\s*([A-Za-z0-9_]+)\s*,\s*([A-Za-z0-9_]+)/) {
+            my ($name, $val_str, $class, $code) = ($1, $2, $3, $4);
+            $val_str =~ s/[\s\(\)]//g;
+            my $val = 0;
+            foreach my $part (split(/\|/, $val_str)) {
+                $val |= hex($part);
+            }
+            push @list, { val => $val, name => $name, class => $class, code => $code };
+        }
+    }
+} elsif ($mode eq "smb2-status") {
+    # Logic from gen_smb2_mapping
+    while (<$in>) {
+        if (m!^#define\s*([A-Za-z0-9_]+)\s+cpu_to_le32[(]([0-9a-fA-Fx]+)[)]\s+//\s+([-A-Z0-9_]+)!) {
+            my ($status, $code_hex, $error) = ($1, $2, $3);
+            next if ($status =~ /^STATUS_SEVERITY/);
+            push @list, { val => hex($code_hex), name => $status, error => $error, code_hex => $code_hex };
+        }
+    }
+} elsif ($mode =~ /^smb1-posix-(ERRDOS|ERRSRV)$/) {
+    # Logic from our previous generate_smberr_posix_map.pl
+    my $target_class = $1;
+    my $current_class = "";
+    while (<$in>) {
+        if (m!generated with the ERRDOS error class!) { $current_class = "ERRDOS"; }
+        elsif (m!generated with the ERRSRV error class!) { $current_class = "ERRSRV"; }
+        next if ($current_class ne $target_class);
+
+        if (m!^#define\s+([A-Za-z0-9_]+)\s+([0-9a-fA-FxX]+)\s*//\s*(\S+)!) {
+            my ($name, $val_str, $error) = ($1, $2, $3);
+            my $val = ($val_str =~ /^0x/i) ? hex($val_str) : $val_str;
+            push @list, { val => $val, name => $name, error => $error };
+        }
+    }
+}
+close($in);
+
+# Universal sorting by numeric value
+@list = sort { $a->{val} <=> $b->{val} } @list;
+
+open(my $out, ">", $out_file) or die "Cannot open $out_file: $!";
+print $out "/* Autogenerated from $in_file by $0 mode $mode */\n\n";
+
+if ($mode eq "smb1-nt") {
+    foreach my $e (@list) {
+        printf $out "\t{ %s, %s, %s, \"%s\" },\n", $e->{class}, $e->{code}, $e->{name}, $e->{name};
+    }
+    print $out "\t{ 0, 0, 0, NULL }\n";
+} elsif ($mode eq "smb2-status") {
+    my $full_status = "";
+    for (my $i = 0; $i < scalar @list; $i++) {
+        my $e = $list[$i];
+        next if ($e->{val} == 0);
+        $full_status .= $e->{name};
+        if ($i < $#list && $list[$i+1]->{val} == $e->{val}) {
+            $full_status .= " or ";
+            next;
+        }
+        printf $out "{ %s, %s, \"%s\" },\n", $e->{code_hex}, $e->{error}, $full_status;
+        $full_status = "";
+    }
+} elsif ($mode =~ /^smb1-posix-/) {
+    foreach my $e (@list) {
+        printf $out "\t{%s, %s},\n", $e->{name}, $e->{error};
+    }
+}
+close($out);
